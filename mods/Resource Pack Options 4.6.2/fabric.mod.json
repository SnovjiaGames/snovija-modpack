      "io.gitlab.jfronny.respackopts.RespackoptsConfig"
    ],
    "modmenu": [
      "io.gitlab.jfronny.respackopts.integration.ModMenuIntegration"
    ]
  },
  "mixins": [
    "respackopts.mixins.json",
    {
      "config": "respackopts.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": "\u003e\u003d0.12.0",
    "fabric-api": "*",
    "minecraft": "*",
    "libjf-base": "\u003e\u003d3.0.0",
