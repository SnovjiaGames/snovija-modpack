{
  "schemaVersion": 1,
  "id": "respackopts",
  "name": "Resource Pack Options",
  "version": "4.6.2",
  "description": "Config menus for resource packs",
  "authors": [
    "JFronny"
  ],
  "contact": {
    "email": "projects.contact@frohnmeyer-wds.de",
    "homepage": "https://jfronny.gitlab.io",
    "issues": "https://git.frohnmeyer-wds.de/JfMods/Respackopts/issues",
    "sources": "https://git.frohnmeyer-wds.de/JfMods/Respackopts"
  },
  "license": "MIT",
  "icon": "assets/respackopts/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "io.gitlab.jfronny.respackopts.Respackopts"
    ],
    "client": [
      "io.gitlab.jfronny.respackopts.RespackoptsClient"
    ],
    "frex": [
      "io.gitlab.jfronny.respackopts.integration.FrexCompat"
    ],
    "respackopts:save_hook": [
      "io.gitlab.jfronny.respackopts.Respackopts"
